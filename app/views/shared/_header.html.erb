     <%#= byebug %>
  <div id="menutop">
    <ul id="menutop-left">
      <li><a href="">Articles</a></li>
      <li><a href="">Contact</a></li>
      <li><a href="">Privacy</a></li>
      <li><a href="">Terms</a></li>
    </ul>
    <ul id="menutop-right">
    <li>
    <% if current_user %>
     <a href="" ><%= current_user.name %>|<%= link_to "Logout",  signout_path%></a>
     <% else %>
     <a href="" data-toggle="modal" data-target="#myModal" >Login | Register</a>
    <% end %>
      <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel"
      aria-hidden="true">
      <div class="modal-dialog modal-lg">
          <div class="modal-content">
              <div class="modal-header" style="background-color: red">
                  <button type="button" class="close" data-dismiss="modal" aria-hidden="true" style="margin-top: -11px">
                      Ã—</button>
              </div>
              <div class="modal-body">
                  <div class="row">
                      <div class="col-md-8" style="border-right: 1px dotted #C2C2C2;padding-right: 30px;">
                          <ul class="nav nav-tabs pull-left">
                              <li class="active"><a href="#Login" data-toggle="tab">Login</a></li>
                              <li><a href="#Registration" data-toggle="tab">Register</a></li>
                          </ul>
                          <div class="tab-content">
                              <div class="tab-pane active" id="Login">
                                 <div style="margin-left: 100px">
                                  <%= form_for :session, url: login_path, method: :post, class: "form-horizontal" do |f| %>
                                  <div class="form-group">
                                      <div class="col-sm-10">
                                        <%= f.text_field :email, class: "form-control",placeholder: "Email" %>

                                      </div>
                                  </div>
                                  <div class="form-group">
                                      <div class="col-sm-10">
                                        <%= f.password_field :password, class: "form-control",placeholder: "Password" %>
                                      </div>
                                  </div>
                                  <div class="form-group">
                                      <div class="col-sm-10">
                                      <%= f.check_box :remmembertoken %> <span style="color: #737373">Remember password?</span>
                                      </div>
                                  </div>
                                  <div class="row">
                                      <div class="col-sm-2">
                                      </div>
                                      <div style="margin-left: 15px">
                                        <div class="col-sm-10">
                                          <%= button_tag "Login", class: "btn btn-primary btn-sm" %>
                                          <a href="javascript:;">Forgot your password?</a>
                                      </div>
                                      </div>
                                  </div>
                                  <% end %>
                                 </div>
                              </div>
                              <div class="tab-pane" id="Registration">
                                  <div style="margin-left: 100px">
                                    <%= form_for :user, url: signup_path, method: :post, class: 'form-horizontal' do |f| %>
                                  <div class="form-group">
                                      <div class="col-sm-10">
                                          <div class="row">
                                              <div class="col-md-12">
                                                  <%= f.text_field :name, class: "form-control",placeholder: "Name" %>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                                  <div class="form-group">
                                      <div class="col-sm-10">
                                                  <%= f.text_field :email, class: "form-control",placeholder: "Email" %>
                                      </div>
                                  </div>
                                  <div class="form-group">
                                      <div class="col-sm-10">
                                                  <%= f.text_field :moblie, class: "form-control",placeholder: "Mobile" %>
                                      </div>
                                  </div>
                                  <div class="form-group">
                                      
                                      <div class="col-sm-10">
                                                  <%= f.text_field :address, class: "form-control",placeholder: "Address" %>
                                      </div>
                                  </div>
                                  <div class="form-group">
                                      
                                      <div class="col-sm-10">
                                                  <%= f.password_field :password, class: "form-control",placeholder: "Password" %>
                                      </div>
                                  </div>
                                  <div class="form-group">
                                      <div class="col-sm-10">
                                                  <%= f.password_field :password_confirmation, class: "form-control",placeholder: "PasswordConfirmation" %>
                                      </div>
                                  </div>
                                  <div class="row">
                                      <div class="col-sm-2">
                                      </div>
                                      <div style="margin-left: 18px">
                                        <div class="col-sm-10">
                                          <%= button_tag "Save & Continue", class: "btn btn-primary btn-sm" %>
                                      </div>
                                      </div>
                                  </div>
                                <% end %>
                                  </div>
                              </div>
                          </div>
                          <div id="OR" class="hidden-xs">
                              OR</div>
                      </div>
                      <div class="col-md-4">
                          <div class="row text-center sign-with">
                              <div class="col-md-12">
                                  <h3>
                                      Sign in with</h3>
                              </div>
                              <div class="col-md-12">
                                  <div class="btn-group btn-group-justified">
                                      <a href="#" class="btn btn-primary">Facebook</a> <a href="#" class="btn btn-danger">
                                          Google</a>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>
      </div>
    </li>
    </ul>
  </div>
